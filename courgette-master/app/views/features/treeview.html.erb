<ul class="jqueryFileTree" style="display: none;">
  <% @directories.each do |dir| %>
      <li class="directory collapsed">
        <a href="treeview.html.erb#" rel="<%= @folder_path + dir %>/"><%= dir %></a></li>
  <% end %>
  <% @files.each do |file| %>
      <li class="file ext_<%= File.extname(file)[1..-1] %>">
        <a href="treeview.html.erb#" rel="<%= @folder_path + file %>"><%= file %></a></li>
  <% end %>
</ul>
